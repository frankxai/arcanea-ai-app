{
  "name": "arcanea-ai",
  "version": "1.0.0",
  "description": "Arcanea.ai - Premium AI-powered spatial worldbuilding experience",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "analyze": "cross-env ANALYZE=true next build",
    "deploy:prod": "npm run build && npm run start",
    "security:audit": "npm audit --audit-level=moderate",
    "performance:check": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-results.json"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@google/generative-ai": "^0.15.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-toast": "^1.1.5",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/xr": "^5.5.0",
    "@supabase/supabase-js": "^2.38.0",
    "@types/three": "^0.160.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.469.0",
    "next": "^16.1.1",
    "openai": "^4.28.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io-client": "^4.7.0",
    "swr": "^2.2.0",
    "tailwind-merge": "^2.0.0",
    "three": "^0.160.0",
    "three-stdlib": "^2.28.0",
    "zod": "^3.25.76",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4"
  }
}
